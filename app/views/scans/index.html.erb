<p id="notice"><%= notice %></p>

<h1>Scans</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Url</th>
      <th>Sitemap</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scans.each do |scan| %>
      <tr>
        <td><%= scan.status %></td>
        <td><%= scan.url %></td>
        <td><%= scan.sitemap %></td>
        <td><%= link_to 'Show', scan %></td>
        <td><%= link_to 'Edit', edit_scan_path(scan) %></td>
        <td><%= link_to 'Destroy', scan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scan', new_scan_path %>
