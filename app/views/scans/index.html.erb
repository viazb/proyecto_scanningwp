<p id="notice"><%= notice %></p>

<div class="container">
  <div class="newscan">
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-sm-11">
            <h1 class="panel-title"> <span class="glyphicon glyphicon-flash"></span>An√°lisis Realizados</h1>
          </div>
          </div>
        </div>
      <div class="panel-body">
        <table class="table">
          <thead>
            <tr>
              <th>Status</th>
              <th>Url</th>
              <th>Creado</th>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody>
            <div class="row">
              <% @scans.each do |scan| %>
                  <td><%= 'Terminado' %></td>
                  <td><%= scan.url %></td>
                  <td><%= scan.created_at %></td>
                  <td><%= link_to 'Ver', scan, class:'btn btn-primary btn-md' %></td>
                  <!--<td><%= link_to 'Edit', edit_scan_path(scan) %></td>-->
                  <td><%= link_to 'Eliminar', scan, method: :delete, data: { confirm: 'Are you sure?' }, class:'btn btn-danger btn-md' %></td>
                </tr>
            </div>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
